From: Tomas Zeman <tzeman@volny.cz>
Subject: [PATCH] tg/dietlibc-update

dietlibc-0.33

---
 pkgs/os-specific/linux/dietlibc/default.nix | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/pkgs/os-specific/linux/dietlibc/default.nix b/pkgs/os-specific/linux/dietlibc/default.nix
index b795cb6..1e41705 100644
--- a/pkgs/os-specific/linux/dietlibc/default.nix
+++ b/pkgs/os-specific/linux/dietlibc/default.nix
@@ -3,10 +3,10 @@
 assert stdenv.isLinux;
 
 stdenv.mkDerivation {
-  name = "dietlibc-0.30";
+  name = "dietlibc-0.33";
   src = fetchurl {
-    url = mirror://kernel/linux/libs/dietlibc/dietlibc-0.30.tar.bz2;
-    md5 = "2465d652fff6f1fad3da3b98e60e83c9";
+    url = http://www.fefe.de/dietlibc/dietlibc-0.33.tar.bz2;
+    sha256 = "1x8mng9h7xljdhnkvdg42hcxrqala11ya1hspi17mp8dg69qi0v8";
   };
   builder = ./builder.sh;
   
@@ -22,11 +22,11 @@ stdenv.mkDerivation {
 
     # wchar.h declares lots of functions that don't actually exist.
     # Remove them.
-    ./no-wchar.patch
+    #./no-wchar.patch
 
     # Fix to get DNS resolution to work on 64-bit platforms.  Taken
     # from 0.31 CVS.
-    ./dns64.patch
+    #./dns64.patch
 
     # Get lseek64 working on x86_64.  From
     # http://svn.annvix.org/cgi-bin/viewvc.cgi/packages/releases/2.0-CURRENT/dietlibc/SOURCES
-- 
tg: (bb6b423..) tg/dietlibc-update (depends on: master)
